<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dangle.easyexams">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/round_pre_good"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/round_pre_good"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dangle.easyexams.MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.dangle.easyexams.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dangle.easyexams.ExamActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.dangle.easyexams.Learn" />
        <activity
            android:name="com.dangle.easyexams.EndGame"
            android:screenOrientation="portrait" />
        <activity android:name="com.dangle.easyexams.WrongAnswersActivity" />
        <activity android:name="com.dangle.easyexams.FullExam" />
        <activity
            android:name="com.dangle.easyexams.PerformanceIndicators"
            android:screenOrientation="portrait" />
        <activity android:name="com.dangle.easyexams.ChooseExam" />
        <activity
            android:name=".SettingsActivity"
            android:parentActivityName="com.dangle.easyexams.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dangle.easyexams.QuestionBankActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.dangle.easyexams.LoginActivity" />
        <activity android:name="com.dangle.easyexams.RegisterActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>